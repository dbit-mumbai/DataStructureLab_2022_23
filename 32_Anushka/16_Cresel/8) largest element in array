#include <stdio.h>

int main()
{
   int n,i,max,a[100];
   printf("Enter Array Size:");
   scanf("%d",&n);
   printf("Enter Array Elements:\n");
   for(i=0;i<n;i++)
   {
       printf("Enter The Number:");
       scanf ("%d",&a[i]);
   }
   printf("The Array is:-");
   for(i=0;i<n;i++)
   {
       printf("%d\t",a[i]);
   }
   max=a[0];
   for(i=1;i<n;i++)
   {
       if(max<a[i])
       {
           max=a[i];
       }
   }
   printf("\nThe Largest Array Element is = %d",max);
}
