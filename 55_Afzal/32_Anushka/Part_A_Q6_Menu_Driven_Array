#include<stdio.h>
#include<stdlib.h>

int compare(int *arr1, int *arr2)
{
    for(int i = 0; i < 5; i++)
    {
        if(arr1[i] != arr2[i])
        {
            return 0;
        }
    }
    return 1;
}


int main()
{
	int arr1[5] = {1,2,3,7,5}; 
    	int arr2[5] = {1,2,3,7,5};
	printf("%d", compare(arr1, arr2))
	return 0;
}
